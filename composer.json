{
	"name": "colymba/colorfield",
	"type": "silverstripe-vendormodule",
	"description": "SilverStripe 4 Color picker and DBField field with attitude.",
	"keywords": ["silverstripe", "silverstripe 4", "color", "colour", "color picker", "colour field", "DBField"],
	"license": "BSD Simplified",
	"authors": [{
		"name": "Thierry Francois",
		"email": "thierry@colymba.com"
	}],
	"require": {
		"php": ">=5.6.0",
		"silverstripe/framework": "^4.0",
		"abeautifulsite/jquery-minicolors": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Colymba\\ColorField\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		},
		"expose": [
			"client/dist",
			"/vendor/abeautifulsite/jquery-minicolors"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
